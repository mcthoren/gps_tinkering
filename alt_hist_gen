#!/usr/bin/perl -T

use strict;
use warnings;
use utf8;
use open qw(:std :utf8);
use Encode qw(encode decode);

@ARGV = map { decode("UTF-8", $_) } @ARGV;

my %el_hash;
my $elev;

while (<>) {
	chomp;
	my ($x, $y, $a, $alt, $b) = split;
	$el_hash{$alt}++ if ($alt > 300 && $alt < 700);
}

foreach $elev (sort keys %el_hash) {
	printf "%.1f m\t%d\n", $elev, $el_hash{$elev};
}
